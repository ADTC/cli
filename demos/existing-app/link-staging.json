          },
          {
            "label": "test partners org",
            "value": "test partners org"
          }
        ]
      }
    },
    {
      "type": "selectPrompt",
      "properties": {
        "message": "Which app would you like to link to?",
        "choices": [
          {
            "label": "Printful App",
            "value": "printful"
          },
          {
            "label": "Printful Dev",
            "value": "development"
          },
          {
            "label": "Printful Staging",
            "value": "staging"
          },
          {
            "label": "Printful (Alice)",
            "value": "alice"
          }
        ]
      }
